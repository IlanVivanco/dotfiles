	PreferredAuthentications publickey
	IdentityFile ~/.ssh/wpengine
	IdentitiesOnly yes
	ServerAliveInterval 60

Host *.drush.in
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/wpe_ssh
	IdentitiesOnly yes

Host ssh.pressable.com
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/pressable
	IdentitiesOnly yes
	ServerAliveInterval 60
